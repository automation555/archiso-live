pkgname=3proxy-win32
_pkgname=3proxy
pkgver=0.6
pkgrel=1
pkgdesc="3proxy tiny free proxy server"
url="http://3proxy.ru/"
groups=('archpwn' 'archpwn-windows')
license=('GPL')
arch=(any)
source=(http://3proxy.ru/current/$_pkgname-$pkgver-devel.zip)
md5sums=('6417d9097e9114cb64121ab5d5483888')

build() {
  cd $srcdir/
  rm *.zip # Remove symlinks
  install -d $pkgdir/opt/windows/$pkgname/ && \
  cp -r * $pkgdir/opt/windows/$pkgname/ || return 1
}

