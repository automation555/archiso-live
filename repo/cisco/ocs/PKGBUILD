# Contributor: Francesco Piccinno <stack.box@gmail.com>

pkgname=ocs
pkgver=0.1
pkgrel=1
pkgdesc="Compact mass scanner for cisco routers with default telnet/enable passwords."
url="http://www.hacklab.tk"
groups=('archpwn' 'archpwn-scanners' 'archpwn-cisco')
license=('GPL')
arch=('i686' 'x86_64')
makedepends=('gcc')
source=(http://packetstormsecurity.org/UNIX/scanners/$pkgname.c)
md5sums=('5ee92e30989dae45e18bf56d7d61ffc9')

build() {
  cd "$srcdir"
  gcc $pkgname.c -o $pkgname -lpthread
  install -d $pkgdir/usr/bin
  install -Dm755 $pkgname $pkgdir/usr/bin/
}

# vim:set ts=2 sw=2 et:
