pkgname=smbclient-impacket
_pkgname=smbclient
pkgver=latest
pkgrel=1
pkgdesc="SMB-Client.py SMB shell"
url="http://oss.coresecurity.com/projects/impacket.html"
groups=('archpwn' 'archpwn-misc')
license=('GPL')
arch=(any)
depends=('python' 'impacket')
source=(http://eckhart.stderr.org/doc/python-impacket-doc/examples/$_pkgname.py)
md5sums=('b8fff033d4ce83b2293986d455e71158')

build() {
  cd "$srcdir"
  install -d $pkgdir/usr/bin || return 1
  install -Dm755 $_pkgname.py $pkgdir/usr/bin/$_pkgname.py || return 1
}

