install ()
{
    MODULES="cdrom ide-cd_mod ide-core ide-generic aufs unionfs squashfs isofs loop ext2 xfs $(all_modules '/kernel/fs' | grep -v "nls") "
    BINARIES="cp ls find touch rm test chmod chown"
    #FILES="/usr/lib/liblinuxlive"
    FiLES=""
    add_dir /real_root
    add_dir /tmpfs
    SCRIPT="archiso"
}

